{
  "name": "p5.layers",
  "version": "1.0.0",
  "description": "Make it easier to use Graphics instances to make layers",
  "homepage": "https://osteele.github.io/p5.libs/p5.layers/",
  "license": "MIT",
  "author": "Oliver Steele <steele@osteele.com> (https://osteele.com/)",
  "keywords": [
    "creative coding",
    "p5",
    "p5.js"
  ],
  "repository": "github:osteele/p5.libs",
  "bugs": {
    "url": "https://github.com/osteele/p5.libs/issues"
  },
  "main": "p5.layers.min.js",
  "files": [
    "p5.layers.js",
    "p5.layers.min.js"
  ],
  "scripts": {
    "build": "rollup --config",
    "build:docs": "jsdoc -c ./jsdoc.js && cp -r docs build && p5 build examples -o build/examples",
    "prepack": "yarn run build",
    "test": "true"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "directories": {
    "example": "examples"
  },
  "devDependencies": {
    "jsdoc": "^3.6.7",
    "p5-server": "^0.6.1",
    "rollup": "^2.56.3"
  }
}
