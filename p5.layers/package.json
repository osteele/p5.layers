{
  "name": "p5.layers",
  "version": "1.0.2",
  "description": "Make it easier to use Graphics instances to make layers",
  "homepage": "https://osteele.github.io/p5.libs/p5.layers/",
  "license": "MIT",
  "author": "Oliver Steele <steele@osteele.com> (https://osteele.com/)",
  "keywords": [
    "creative coding",
    "p5",
    "p5.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/osteele/p5.libs.git",
    "directory": "p5.layers"
  },
  "bugs": {
    "url": "https://github.com/osteele/p5.libs/issues"
  },
  "main": "p5.layers.min.js",
  "files": [
    "p5.layers.js",
    "p5.layers.min.js"
  ],
  "scripts": {
    "build": "rollup --config",
    "build:docs": "rimraf build && jsdoc -c ./jsdoc.js && copyfiles 'docs/**/*' build && p5 build examples -o build/examples",
    "prepack": "yarn run build",
    "test": "true"
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "directories": {
    "example": "examples"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "jsdoc": "^3.6.7",
    "p5-server": "^0.7.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3"
  }
}
